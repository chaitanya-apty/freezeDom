{
    "name": "Freeze",
    "version": "1.0",
    "manifest_version" : 2,
    "description": "Prevents Web Actions When Clicked on Web page (Be Careful!)",
    "author": "Chaitanya Kumar",
    "content_scripts": [
        {
          "matches": ["https://*/**", "http://*/**"],
          "js": ["js/freeze.js"]
        }
    ],
    "browser_action": {
      "default_icon": "images/128x128.png"
    },
    "icons" : {
        "16": "images/16x16.png",
        "32": "images/32x32.png",
        "64": "images/64x64.png",
        "128": "images/128x128.png"
    },
    "permissions": [
      "activeTab",  
      "storage"
    ]
  }
  